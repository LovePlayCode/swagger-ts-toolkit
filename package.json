{
  "name": "swagger-ts-toolkit",
  "version": "3.0.1",
  "description": "功能强大的 Swagger/OpenAPI TypeScript 工具包 - 自动生成类型定义、API接口和端点常量",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "swagger-ts-toolkit": "./dist/cli.js",
    "stt": "./dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "node --test",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "release": "./scripts/bump-version.sh"
  },
  "keywords": [
    "swagger",
    "openapi",
    "typescript",
    "code-generator",
    "api",
    "toolkit",
    "types",
    "endpoints",
    "client",
    "codegen"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/swagger-ts-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/swagger-ts-toolkit/issues"
  },
  "homepage": "https://github.com/yourusername/swagger-ts-toolkit#readme",
  "dependencies": {
    "js-yaml": "^4.1.0",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "openapi-typescript": "^6.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
